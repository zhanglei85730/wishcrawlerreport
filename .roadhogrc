{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "publicPath": "/",
   "theme": {
    "@primary-color": "#28bba2",
    "@link-color": "#fff",
    "@border-radius-base": "2px",
    "@font-size-base": "16px",
    "@line-height-base": "1.2"
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
          ["import", { "libraryName": "antd", "style": true }]
      ]
    },
   
   "autoprefixer": null,
    "proxy": {
      "/api": {
        "target": "http://localhost:3000/",
        "changeOrigin": true,
        "pathRewrite": { "^/api" : "" }
      }
    },
   "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
    "production": {
      "extraBabelPlugins": [
        "transform-runtime"
      ]
    }
  }
}
